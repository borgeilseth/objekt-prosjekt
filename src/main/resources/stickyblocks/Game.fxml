               <items>
                  <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <HBox alignment="CENTER" prefHeight="720.0" prefWidth="1280.0">
        <children>
            
            <Canvas fx:id="canvas" focusTraversable="true" height="600.0" width="900.0" onKeyPressed="#handleKeyPressed" />
      
        </children>
